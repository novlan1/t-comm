{
  "tags": {
      "allowUnknownTags": false
  },
  "source": {
      "include": "src",
      "includePattern": "\\.(t|j)s$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown",
      "node_modules/better-docs/typescript"
    ],
  "opts": {
      "template": "node_modules/docdash/",
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "docdash": {
    "static": true,
    "sort": false,
    "disqus": "",
    "search": true,
    "collapse": true,
    "typedefs": true,
    "commonNav": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu":{
        "Github repo":{
            "href":"https://github.com/novlan1/t-comm",
            "target":"_blank",
            "class":"menu-item",
            "id":"repository"
        }
    }
  }
}
