{
  "name": "t-comm",
  "version": "0.0.53",
  "description": "工具库",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "script"
  ],
  "sideEffects": "false",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/novlan1/t-comm.git"
  },
  "author": "ygw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/novlan1/t-comm/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/novlan1/t-comm#readme",
  "scripts": {
    "build": "gulp build",
    "api": "api-extractor run",
    "docs:gen": "jsdoc -c jsdoc.json || true",
    "test": "jest --coverage --verbose -u",
    "lint": "eslint --ext .js,.ts --format=pretty ./src",
    "lint:fix": "eslint --fix --ext .js,.ts --format=pretty ./src",
    "changelog": "gulp changelog",
    "version": "yarn changelog && git add CHANGELOG.md",
    "release": "yarn test && yarn build && np --no-cleanup --yolo --any-branch",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@microsoft/api-extractor": "^7.19.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@yueqing/lint": "^2.1.0",
    "chalk": "^4.1.2",
    "conventional-changelog": "^3.1.25",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "np": "^7.6.0",
    "rollup": "^2.67.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@babel/runtime-corejs3": "^7.18.0",
    "axios": "^0.26.1",
    "better-docs": "^2.7.2",
    "docdash": "^1.2.0",
    "jsdoc": "^3.6.11"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --format=pretty"
    ]
  }
